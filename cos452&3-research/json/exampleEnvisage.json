            "code": "COS 311",
            "name": "Ethics in Computer Science",
            "hours": "3",
            "scheduleOptions": [
                "Unspecified",
                "Every Semester",
                "Fall",
                "Spring",
                "J-Term",
                "Summer",
                "Fall Odd Years",
